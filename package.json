{
  "name": "electron-donuts",
  "version": "1.0.0",
  "description": "Electron Donuts",
  "main": "main.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "start": "electron ."
  },
  "build": {
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thesourorange/electron-donuts.git"
  },
  "keywords": [
    "Electron",
    "Donut",
    "Analysist"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "jquery": "^3.4.1",
    "papaparse": "^4.6.0",
    "w2ui": "^1.4.3",
    "node": "^16.9.1"
  },
  "devDependencies": {
    "electron": "^14.0.1",
    "electron-builder": "^22.11.7"
  },
  "bugs": {
    "url": "https://github.com/thesourorange/electron-donuts/issues"
  },
  "homepage": "https://github.com/thesourorange/electron-donuts#readme"
}
